{
    "name": "popov/datagrid-bundle",
    "description": "This bundle allows you to integrate ZfcDatagrid into your Symfony project",
    "keywords": [],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Serhii Popov",
            "email": "popow.serhii@gmail.com"
        }
    ],
    "require": {
        "doctrine/orm": "~2.4",
        "symfony/framework-bundle": "~4.0|~5.0|~6.0|~7.0",
        "nyholm/psr7": "~1.8",
        "symfony/psr-http-message-bridge": "~4.0|~5.0|~6.0|~7.0",
        "symfony/console": "~4.0|~5.0|~6.0|~7.0",
        "symfony/process": "~4.0|~5.0|~6.0|~7.0",
        "symfony/yaml": "~4.0|~5.0|~6.0|~7.0",
        "twig/twig": "^1.28.2|^2.0|^3.0",
        "laminas/laminas-serializer": "^3.0"
    },
    "require-dev": {
        "symfony/doctrine-bridge": "~4.0|~5.0",
        "symfony/phpunit-bridge": "^3.3"
    },
    "autoload": {
        "psr-4": {
          "Popov\\DatagridBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
